  "dependencies": {
    "chatapp.api-error": "workspace:*",
    "chatapp.logging": "workspace:*",
    "chatapp.middlewares": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-fileupload": "^1.5.1",
    "express": "^5.0.1",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1"
  }
}
